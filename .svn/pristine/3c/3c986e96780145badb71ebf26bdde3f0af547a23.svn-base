import request from '@/utils/request'

//技能详情
export function skillDetail(id) {
  return request({
    url: `market/skillMarket/selectSkill?skillId=${id}`,
    method: 'get',
  })
}
//机器人详情
export function robotDetail(id) {
  return request({
    url: `market/robotMarket/selectRobot?robotId=${id}`,
    method: 'get',
  })
}
